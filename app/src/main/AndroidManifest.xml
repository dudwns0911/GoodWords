<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sample.goodwords">

    <application
        android:allowBackup="true"
        android:icon="@drawable/maintoast"
        android:label="@string/app_name"
        android:roundIcon="@drawable/maintoast"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.sample.goodwords.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sample.goodwords.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name="com.sample.goodwords.GoodwordsActivity"/>
        <activity android:name="com.sample.goodwords.EnglishGoodwordsActivity"/>
    </application>

</manifest>